{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Simulation/Vivarium.csproj"
          ],
          "xml": "src/Simulation/bin/Release/net10.0/Vivarium.xml"
        }
      ],
      "dest": "docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["docs/**/*.md", "docs/**/*.yml"],
        "exclude": [ "docs/**/obj/**", "docs/**/_site/**" ]
      }
    ],
    "resource": [
      {
        "files": ["docs/images/**"]
      }
    ],
    "dest": "docs/_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appTitle": "Vivarium",
      "_appName": "Vivarium",
      "_appFooter": "Copyright Â© 2026",
      "_enableSearch": true
    }
  }
}