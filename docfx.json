{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Simulation/Vivarium.csproj"
          ],
          "xml": "src/Simulation/bin/Release/net10.0/Vivarium.xml"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "allowGeneration": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.yml",
          "api/*.json",
          "docs/toc.yml",
          "docs/index.md",
          "docs/*.md",
          "README.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ]
      }
    ],
    "dest": "_site",
    "siteBaseUrl": "https://kromtec.github.io/Vivarium",
    "template": [
      "default"
    ],
    "globalMetadata": {
      "_appTitle": "Vivarium",
      "_appName": "Vivarium",
      "_appFooter": "Copyright Â© 2026",
      "_enableSearch": true
    }
  }
}
